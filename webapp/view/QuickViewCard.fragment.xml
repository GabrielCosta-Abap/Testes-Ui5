<core:FragmentDefinition
 xmlns="sap.m"
 xmlns:core="sap.ui.core"
 xmlns:control="test.teste.controller.DragNDrop.controls"
 xmlns:dnd="sap.ui.core.dnd" 
>
	<!-- <control:Panel
		width="auto"> -->
		<control:QuickViewCard
			navigate=".onNavigate"
			
			afterNavigate=".onAfterNavigate">

			<control:dragDropConfig>
				<!-- <dnd:DropInfo drop="onDrop"/> -->
				<dnd:DragInfo drag="onDrop"/>
			</control:dragDropConfig>

			<QuickViewPage
				header="header"
                width="200%"
				title="{FirstName}"
				titleUrl="titleUrl"
				description="description">
				<avatar>
					<Avatar
						src="icon"
						displayShape="displayShape"
					/>
				</avatar>
				<QuickViewGroup heading="heading">
					<QuickViewGroupElement
						label="label"
						value="value"
						url="url"
						type="elementType"
						pageLinkId="pageLinkId"
						emailSubject="emailSubject"
						target="target" />
				</QuickViewGroup>
			</QuickViewPage>
		</control:QuickViewCard>
	<!-- </control:Panel> -->
</core:FragmentDefinition>